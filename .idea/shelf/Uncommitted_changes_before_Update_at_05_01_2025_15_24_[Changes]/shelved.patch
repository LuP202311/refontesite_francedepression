<+>:root {\r\n    --color-white: white;\r\n    --color-black: black;\r\n\r\n}\r\n\r\n* {\r\n    padding: 0;\r\n    margin: 0;\r\n    box-sizing: border-box /*gere les depassements et les dimensions des elements*/\r\n}\r\n\r\nbody{\r\n    font-family: 'Inter', sans-serif;\r\n}\r\n\r\n\r\nmain{\r\n    min-height: 80vh;\r\n    position: relative;\r\n    margin : 2em 4em 4em 4em;\r\n}\r\n\r\n/* ----------- BANNIERE ------------- */\r\n\r\n.banniere{\r\n    width: 100%;\r\n    height: 150px;\r\n}\r\n\r\n/*------------- FIN BANNIERE -------------*/\r\n\r\n/* CHEMIN */\r\n.chemin{\r\n    margin : 0 0 2em 0;\r\n    text-decoration: none;\r\n    font-size: large;\r\n}\r\n.chemin a{\r\n    color: black;\r\n    text-decoration: none;\r\n}\r\n/* FIN CHEMIN */\r\n\r\n/* BARRE DE NAVIGATION */\r\nheader {\r\n    position: sticky;\r\n    top: 0;\r\n    z-index: 10;\r\n    background-color: var(--color-white);\r\n}\r\n\r\n.sidenav{\r\n    display: none;\r\n}\r\n\r\nnav {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n}\r\n\r\n/*PARTIE GAUCHE*/\r\n.divLogo {\r\n    display: flex;\r\n    align-items: center;\r\n    white-space: nowrap;\r\n}\r\n\r\n.divLogo a {\r\n    text-decoration: none;\r\n}\r\n\r\n.divLogo span {\r\n    font-size: 0.938em;\r\n    color: #506FA5;\r\n}\r\n\r\n/*PARTIE CENTRE*/\r\n.navbar ul {\r\n    list-style-type: none;\r\n    display: flex;\r\n    align-items: center;\r\n}\r\n\r\n.navbar ul .section {\r\n    margin: 0 4.375rem;\r\n    text-align: center;\r\n    display: inline;\r\n    position: relative;\r\n    font-size: 1.188em;\r\n}\r\n\r\n.navbar a {\r\n    color: #506FA5;\r\n    text-decoration: none;\r\n}\r\n\r\n.navbar a:hover {\r\n    color: #bcd6f7;\r\n}\r\n\r\n#drop{\r\n    padding: 1.5em 0 1.563em 0;\r\n}\r\n\r\n#dropDown{\r\n    display: none;\r\n    position: absolute;\r\n    background-color: #bfe8f4;\r\n    border-radius: 0.938em;\r\n    margin-top: 1.563em;\r\n}\r\n\r\n#dropDown li{\r\n    padding: 0.625em;\r\n}\r\n\r\n#dropDown a{\r\n    color: black;\r\n}\r\n\r\n#drop:hover #dropDown{\r\n    display: block;\r\n    white-space: nowrap;\r\n}\r\n\r\n#dropDown li:hover {\r\n    background-color: #70defe;\r\n    border-radius: 0.938em;\r\n}\r\n\r\n\r\n/*PARTIE DROITE*/\r\n#login {\r\n    margin-right: 6.25em;\r\n    display: flex;\r\n    align-items: center;\r\n}\r\n\r\n#login:hover{\r\n    opacity: 0.3; /*transparence*/\r\n}\r\n\r\n/*MENU BURGER*/\r\n/* Sous-menu caché par défaut */\r\n.dropD {\r\n    display: none;\r\n    margin-left: 20px;\r\n}\r\n\r\n.dropD li a {\r\n    font-size: 17px;\r\n    color: #111;\r\n}\r\n.dropD li a:hover {\r\n    color: #888787;\r\n}\r\n\r\n#logoBurger{\r\n    position: absolute;\r\n    margin-left: 20%;\r\n    margin-right: 20%;\r\n    bottom: 2em;\r\n}\r\n/*FIN MENU BURGER*/\r\n\r\n/*FIN DE LA BARRE DE NAVIGATION*/\r\n\r\n/*BOUTON POUR REMONTER*/\r\n#retourHaut {\r\n    display: none;\r\n    position: fixed;\r\n    bottom: 14em;\r\n    right: 1.25em;\r\n    z-index: 2;\r\n    padding: 1em 1.25em;\r\n    cursor: pointer;\r\n    width: 5em;\r\n\r\n}\r\n\r\n#retourHaut:hover {\r\n    opacity: 0.5;\r\n}\r\n/*FIN DU BOUTON*/\r\n\r\n/*BOUTON QUESTIONNAIRE*/\r\n.form {\r\n    position: relative;\r\n}\r\n\r\n.btnForm{\r\n    position: sticky;\r\n    top: 10px;\r\n}\r\n\r\n.btnForm button{\r\n    padding: 0.7rem 1rem;\r\n    font-size: 1em;\r\n    background-color: #f95c40;\r\n    color: var(--color-white);\r\n    border: none;\r\n    border-radius: 0.5rem;\r\n    cursor: pointer; /*Change le curseur*/\r\n}\r\n\r\n.btnForm button:hover {\r\n    background-color: #af0505;\r\n    transition: background-color 0.3s ease; /*Temps de la transition*/\r\n}\r\n/*FIN BOUTON QUESTIONNAIRE*/\r\n\r\n/*FOOTER*/\r\nfooter {\r\n    background-color: #A3C7C2;\r\n    width: 100%;\r\n}\r\n\r\n.divFooter {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: flex-start;\r\n    padding: 1.5em 6em;\r\n}\r\n\r\n.titre-footer{\r\n    color: #6C6873;\r\n    font-size: 1.25em;\r\n}\r\n\r\n.contenu{\r\n    color: var(--color-white);\r\n    font-size: 0.875em;\r\n    margin: 0;\r\n    line-height: 1.5;\r\n}\r\n\r\n.contenu a{\r\n    text-decoration: none;\r\n    color: var(--color-white);\r\n}\r\n\r\nfooter a:hover{\r\n    text-decoration: underline;\r\n}\r\n\r\n\r\n.mention a,\r\n.mention p{\r\n    color: var(--color-white);\r\n    text-align: center;\r\n    text-decoration: none;\r\n}\r\n/*FIN DU FOOTER*/\r\n\r\n/*MEDIA QUERIES*/\r\n@media screen and (max-width: 1457px)\r\n{\r\n    .navbar{\r\n        display: none;\r\n    }\r\n    /* Sidenav menu */\r\n    .sidenav {\r\n        order: -1;\r\n        display: block;\r\n        height: 100%;\r\n        width: 250px;\r\n        position: fixed;\r\n        z-index: 1;\r\n        top: 0;\r\n        left: -250px;\r\n        background-color: #f0f0f2;\r\n        padding-top: 60px;\r\n        transition: left 0.5s ease;\r\n    }\r\n\r\n    /* Sidenav menu links */\r\n    .sidenav a {\r\n        padding: 8px 8px 8px 32px;\r\n        text-decoration: none;\r\n        font-size: 25px;\r\n        color: #708090;\r\n        display: block;\r\n        transition: 0.3s;\r\n    }\r\n\r\n    .sidenav a:hover {\r\n        color: #b0c4de;\r\n    }\r\n\r\n    .sidenav ul {\r\n        list-style-type: none;\r\n        padding: 0;\r\n        margin: 0;\r\n    }\r\n\r\n    /* Active class */\r\n    .sidenav.active {\r\n        left: 0;\r\n    }\r\n\r\n    /* Close btn */\r\n    .sidenav .close {\r\n        position: absolute;\r\n        top: 0;\r\n        right: 25px;\r\n        font-size: 36px;\r\n    }\r\n\r\n    .openBtn{\r\n        margin-left: 4em;\r\n    }\r\n\r\n    /* Icône burger */\r\n    .burger-icon span {\r\n        display: block;\r\n        width: 35px;\r\n        height: 5px;\r\n        background-color: black;\r\n        margin: 6px\r\n    }\r\n\r\n}\r\n\r\n@media (min-width: 601px) and (max-width: 1310px) {\r\n    /*FOOTER*/\r\n    .divFooter {\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        justify-content: center;\r\n        gap: 2.5rem;\r\n    }\r\n\r\n    .divLogo img {\r\n        display: block;\r\n        margin: 0 auto;\r\n    }\r\n\r\n    .titre-footer {\r\n        font-size: 1em;\r\n    }\r\n\r\n    .contenu{\r\n        font-size: 0.85em;\r\n    }\r\n    /*FIN FOOTER*/\r\n}\r\n\r\n/*FORMAT PETIT ECRAN*/\r\n@media (max-width: 600px) {\r\n    main{\r\n        margin: 1em 2em 2em 2em;\r\n    }\r\n    \r\n    /*FOOTER*/\r\n    .divFooter {\r\n        flex-direction: column;\r\n        align-items: center;\r\n        gap: 1rem;\r\n        text-align: center; /*Aligne le texte au centre*/\r\n        width: 100%; /*Assure qu'il prenne toute la largeur de l’écran */\r\n    }\r\n\r\n    .divLogo img {\r\n        display: block;\r\n        margin: 0 auto;\r\n    }\r\n\r\n    .contenu, .titre-footer {\r\n        font-size: 1em;\r\n    }\r\n    /*FIN FOOTER*/\r\n}\r\n/*FIN FORMAT PETIT ECRAN*/\r\n/*FIN MEDIA QUERIES*/\r\n
